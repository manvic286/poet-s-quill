# Example environment variables for local development
# Copy this file to `.env` and fill the values with your project's credentials

# Client-accessible (exposed to browser)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Server-side (do NOT commit these to source control in a real project)
# SUPABASE_URL can be set to the same value as NEXT_PUBLIC_SUPABASE_URL
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Required for admin operations (createUser, service-side admin actions)
# Keep this secret and set it in your deployment provider's secret manager
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
